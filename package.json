{
    "name": "@paywatchglobal/n8n-nodes-sharepoint-files",
    "version": "0.1.0",
    "description": "n8n community node to manage files and folders in SharePoint sites via Microsoft Graph API",
    "license": "MIT",
    "homepage": "",
    "keywords": [
        "n8n-community-node-package"
    ],
    "author": {
        "name": "Asyrique Thevendran",
        "email": "asyrique.thevendran@paywatchglobal.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/paywatchglobal/n8n-nodes-sharepoint-files.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": true,
        "credentials": [
            "dist/credentials/SharePointFilesApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/SharePointFiles/SharePointFiles.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "^0.19.0",
        "@types/node": "^25.2.3",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
